{
    "name": "qubao-tool-kit",
    "displayName": "qubao-tool-kit",
    "description": "qubao took kit for vscode",
    "version": "0.0.1",
    "engines": {
        "vscode": "^1.34.0"
    },
    "categories": [
        "Other"
    ],
    "activationEvents": [
        "workspaceContains:**/*"
    ],
    "main": "./out/extension.js",
    "contributes": {
        "configuration": {
            "type": "object",
            "description": "configuration for qubao took kit",
            "properties": {
                "qtk": {
                    "type": "object",
                    "description": "the configuration of qubao tool kit",
                    "cpr": {
                        "type": "object",
                        "description": "cpr config for every folder",
                        "patternProperties": {
                            ".+": {
                                "type": "object",
                                "properties": {
                                    "targetId": {
                                        "type": "string"
                                    },
                                    "list": {
                                        "type": "array",
                                        "additionalItems": false,
                                        "items": {
                                            "type": "object",
                                            "properties": {
                                                "remoteAddr": {
                                                    "type": "string"
                                                },
                                                "remoteUser": {
                                                    "type": "string"
                                                },
                                                "remoteDir": {
                                                    "type": "string"
                                                },
                                                "id": {
                                                    "type": "string"
                                                }
                                            },
                                            "required": [
                                                "remoteAddr",
                                                "remoteUser",
                                                "remoteDir",
                                                "id"
                                            ]
                                        }
                                    }
                                },
                                "required": [
                                    "targetId",
                                    "list"
                                ]
                            }
                        }
                    },
                    "pmr": {
                        "type": "object",
                        "description": "pmr config for every remote server",
                        "properties": {
                            "targetId": {
                                "type": "string",
                                "description": "target config"
                            },
                            "list": {
                                "type": "array",
                                "additionalItems": false,
                                "items": {
                                    "type": "object",
                                    "properties": {
                                        "id": {
                                            "type": "string"
                                        },
                                        "remoteUser": {
                                            "type": "string"
                                        },
                                        "remoteAddr": {
                                            "type": "string"
                                        }
                                    },
                                    "required": [
                                        "id",
                                        "remoteUser",
                                        "remoteAddr"
                                    ]
                                }
                            }
                        },
                        "required": [
                            "targetId",
                            "list"
                        ]
                    },
                    "jump": {
                        "type": "object",
                        "properties": {
                            "defaultPoint": {
                                "type": "string"
                            }
                        }
                    }
                }
            }
        },
        "commands": [
            {
                "command": "extension.qtk.cpr.exec",
                "title": "CpR"
            },
            {
                "command": "extension.qtk.cpr.reset",
                "title": "CpR-Reset"
            },
            {
                "command": "extension.qtk.pmr.exec",
                "title": "PmR"
            },
            {
                "command": "extension.qtk.pmr.reset",
                "title": "PmR-Reset"
            },
            {
                "command": "extension.qtk.jump",
                "title": "jump"
            }
        ],
        "menus": {
            "editor/title": [
                {
                    "when": "editorFocus",
                    "command": "extension.qtk.cpr.exec",
                    "group": "navigation"
                },
                {
                    "when": "editorFocus",
                    "command": "extension.qtk.pmr.exec",
                    "group": "navigation"
                }
            ],
            "editor/context": [
                {
                    "when": "editorFocus",
                    "command": "extension.qtk.jump",
                    "group": "navigation"
                }
            ],
            "explorer/context": [
                {
                    "command": "extension.qtk.jump",
                    "group": "navigation"
                }
            ]
        }
    },
    "scripts": {
        "vscode:prepublish": "yarn run compile",
        "compile": "tsc -p ./",
        "watch": "tsc -watch -p ./",
        "pretest": "yarn run compile",
        "test": "node ./out/test/runTest.js"
    },
    "devDependencies": {
        "@types/glob": "^7.1.1",
        "@types/mocha": "^5.2.7",
        "@types/node": "^12.11.7",
        "@types/vscode": "^1.41.0",
        "glob": "^7.1.5",
        "mocha": "^6.2.2",
        "tslint": "^5.20.0",
        "typescript": "^3.6.4",
        "vscode-test": "^1.2.2"
    },
    "dependencies": {}
}
