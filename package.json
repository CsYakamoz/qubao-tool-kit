{
    "name": "qubao-tool-kit",
    "displayName": "Qubao Tool Kit",
    "description": "qubao took kit for vscode",
    "version": "0.0.7",
    "publisher": "CsYakamoz",
    "engines": {
        "vscode": "^1.34.0"
    },
    "repository": {
        "url": "https://github.com/CsYakamoz/qubao-tool-kit.git"
    },
    "categories": [
        "Other"
    ],
    "activationEvents": [
        "workspaceContains:**/*"
    ],
    "main": "./out/extension.js",
    "contributes": {
        "configuration": {
            "type": "object",
            "description": "configuration for qubao took kit",
            "properties": {
                "qtk": {
                    "type": "object",
                    "description": "the configuration of qubao tool kit",
                    "properties": {
                        "cpr": {
                            "type": "object",
                            "description": "cpr config for every folder",
                            "patternProperties": {
                                ".+": {
                                    "type": "object",
                                    "properties": {
                                        "targetId": {
                                            "type": "string",
                                            "description": "target config"
                                        },
                                        "list": {
                                            "type": "array",
                                            "additionalItems": false,
                                            "items": {
                                                "type": "object",
                                                "properties": {
                                                    "remoteAddr": {
                                                        "type": "string"
                                                    },
                                                    "remoteUser": {
                                                        "type": "string"
                                                    },
                                                    "remoteDir": {
                                                        "type": "string"
                                                    },
                                                    "id": {
                                                        "type": "string"
                                                    }
                                                },
                                                "required": [
                                                    "remoteAddr",
                                                    "remoteUser",
                                                    "remoteDir",
                                                    "id"
                                                ]
                                            }
                                        }
                                    },
                                    "required": [
                                        "targetId",
                                        "list"
                                    ]
                                }
                            }
                        },
                        "pmr": {
                            "type": "object",
                            "description": "pmr config for every remote server",
                            "properties": {
                                "targetId": {
                                    "type": "string",
                                    "description": "target config"
                                },
                                "list": {
                                    "type": "array",
                                    "additionalItems": false,
                                    "items": {
                                        "type": "object",
                                        "properties": {
                                            "id": {
                                                "type": "string"
                                            },
                                            "remoteUser": {
                                                "type": "string"
                                            },
                                            "remoteAddr": {
                                                "type": "string"
                                            },
                                            "regex": {
                                                "type": "string"
                                            }
                                        },
                                        "required": [
                                            "id",
                                            "remoteUser",
                                            "remoteAddr"
                                        ]
                                    }
                                }
                            },
                            "required": [
                                "targetId",
                                "list"
                            ]
                        },
                        "switching": {
                            "type": "object",
                            "properties": {
                                "defaultPoint": {
                                    "type": "string",
                                    "description": "default jump to the file if exists"
                                },
                                "viewColumn": {
                                    "type": "integer",
                                    "description": "where to open the file",
                                    "default": -1,
                                    "enum": [
                                        -2,
                                        -1,
                                        1,
                                        2,
                                        3,
                                        4,
                                        5,
                                        6,
                                        7,
                                        8
                                    ]
                                },
                                "preview": {
                                    "type": "boolean",
                                    "default": true,
                                    "description": "An optional view column in which the editor should be shown"
                                },
                                "preserveFocus": {
                                    "type": "boolean",
                                    "default": false,
                                    "description": "An optional flag that when true will stop the editor from taking focus"
                                }
                            }
                        },
                        "exchange": {
                            "type": "object",
                            "properties": {
                                "color": {
                                    "type": "string",
                                    "default": "#FFBD2A",
                                    "description": "Highlight color"
                                }
                            }
                        }
                    }
                }
            }
        },
        "commands": [
            {
                "command": "extension.qtk.cpr.exec",
                "title": "CpR"
            },
            {
                "command": "extension.qtk.cpr.reset",
                "title": "CpR-Reset"
            },
            {
                "command": "extension.qtk.pmr.exec",
                "title": "PmR"
            },
            {
                "command": "extension.qtk.pmr.reset",
                "title": "PmR-Reset"
            },
            {
                "command": "extension.qtk.switching",
                "title": "Switching"
            },
            {
                "command": "extension.qtk.exchange.exec",
                "title": "Exchange"
            },
            {
                "command": "extension.qtk.exchange.clear",
                "title": "ExchangeClear"
            },
            {
                "command": "extension.qtk.exchange.exec.line",
                "title": "ExchangeLine"
            }
        ],
        "menus": {
            "editor/title": [
                {
                    "when": "editorFocus",
                    "command": "extension.qtk.cpr.exec",
                    "group": "navigation"
                },
                {
                    "command": "extension.qtk.pmr.exec",
                    "group": "navigation"
                },
                {
                    "when": "editorFocus",
                    "command": "extension.qtk.switching",
                    "group": "navigation"
                }
            ],
            "editor/context": [
                {
                    "when": "editorFocus",
                    "command": "extension.qtk.switching",
                    "group": "navigation"
                },
                {
                    "when": "editorFocus",
                    "command": "extension.qtk.cpr.exec",
                    "group": "navigation"
                },
                {
                    "when": "editorFocus",
                    "command": "extension.qtk.pmr.exec",
                    "group": "navigation"
                },
                {
                    "when": "editorHasSelection",
                    "command": "extension.qtk.exchange.exec",
                    "group": "navigation"
                }
            ],
            "explorer/context": [
                {
                    "command": "extension.qtk.switching",
                    "group": "2_workspace"
                }
            ]
        }
    },
    "scripts": {
        "vscode:prepublish": "yarn run compile",
        "compile": "tsc -p ./",
        "watch": "tsc -watch -p ./",
        "pretest": "yarn run compile",
        "test": "node ./out/test/runTest.js"
    },
    "devDependencies": {
        "@types/glob": "^7.1.1",
        "@types/mocha": "^5.2.7",
        "@types/node": "^12.11.7",
        "@types/vscode": "^1.34.0",
        "glob": "^7.1.5",
        "mocha": "^6.2.2",
        "tslint": "^5.20.0",
        "typescript": "^3.6.4",
        "vscode-test": "^1.2.2"
    },
    "dependencies": {}
}
